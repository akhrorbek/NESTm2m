datasource db {
    provider = "postgresql"
    url = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}


enum Gender {
    male    @map("male")
    female  @map("female")

    @@map("gender")
}

model Employees {
    id      Int      @map("emp_id")      @db.Integer            @default(autoincrement())
    name    String      @map("emp_name")    @db.VarChar(45)



    @@id([id])
    @@map("employees")
}